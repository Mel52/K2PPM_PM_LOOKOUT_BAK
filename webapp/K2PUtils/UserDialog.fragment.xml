<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialog"
		title="Users"
		items="{/Users}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">
		<StandardListItem
			title="{UserCode}"
			description="{UserName}" />
	</SelectDialog>
</core:FragmentDefinition>